<?php

use Drupal\Core\Form\FormStateInterface;

/**
 * Implements hook_form_alter().
 */
function niauditoffice_common_form_alter(&$form, FormStateInterface $form_state, $form_id) {
  if ($form_id == 'node_publication_page_form' || $form_id == 'node_publication_page_edit_form') {
    $form['field_publication_type']['#disabled'] = 'disabled';
  }
}
